{
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        }
    },
    "coc.preferences.useQuickfixForLocations": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "rust"
    ],
    "git.enableGutters": false
}
